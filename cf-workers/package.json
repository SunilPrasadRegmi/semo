{
  "name": "cloudflare-workers",
  "private": true,
  "scripts": {
    "dev:android-client": "wrangler dev -c workers/android-client/wrangler.toml",
    "dev:zip-to-vtt": "wrangler dev -c workers/zip-to-vtt/wrangler.toml",
    "deploy:android-client": "wrangler deploy -c workers/android-client/wrangler.toml",
    "deploy:zip-to-vtt": "wrangler deploy -c workers/zip-to-vtt/wrangler.toml",
    "dev:all": "npm run dev:android-client & npm run dev:zip-to-vtt",
    "deploy:all": "npm run deploy:android-client && npm run deploy:zip-to-vtt"
  },
  "devDependencies": {
    "wrangler": "^3.78.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@zip.js/zip.js": "^2.7.52"
  }
}
